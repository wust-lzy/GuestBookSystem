
@{
    ViewBag.Title = "CheckMessage";
}

<h2>是否审核通过？</h2>
<p>留言人邮箱：@Model.AuthorEmail</p>
<p>内容：@Model.Content</p>
<p>时间：@Model.CreatedOn</p>
@using (Html.BeginForm())
{
    <input type="submit" value="确认通过" />
    <input type="button" value="取消" onclick="window.location.href= '/Admin/CheckIndex'">
}



